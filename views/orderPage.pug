    <section>

        <div class="booking-main">
            <h4 class="mt-2 p-3" style="font-family: handlee;">Order Form</h4>
            <form name="bookingForm" class="row g-3 p-3 " action = "/product/ordered/"  method="post">
                <div class="col-md-6">
                    <label for="name" class="form-label fw-bold">Name:</label>
                    <input type="text" class="form-control" name="name" value="" id="name">
                    <p id="name_error" class="error"></p>
                </div>
                <div class="col-md-6">
                    <label for="mail" class="form-label fw-bold">E-Mail:</label>
                    <input type="E-mail" class="form-control" id="mail" name="email">
                    <p id="email_error" class="error"></p>
                </div>
                <div class="col-md-6">
                    <label for="NUmber" class="form-label fw-bold">Number:</label>
                    <input type="Number" class="form-control" id="Number" name="number">
                    <p id="number_error" class="error"></p>
                </div>
                <div class="col-md-6">
                    <label for="NUmber" class="form-label fw-bold">Housenumber :</label>
                    <input type="text" class="form-control" id="Housenumber" name="Housenumber">
                    <p id="Housenumber_error" class="error"></p>
                </div>
                <div class="col-md-6">
                    <label for="NUmber" class="form-label fw-bold">colony:</label>
                    <input type="text" class="form-control" id="colony" name="colony">
                    <p id="colony_error" class="error"></p>
                </div>
                <div class="col-md-6">
                    <label for="NUmber" class="form-label fw-bold">landmark:</label>
                    <input type="text" class="form-control" id="landmark" name="landmark">
                    <p id="landmark_error" class="error"></p>
                </div>
                <div class="col-md-6">
                    <label for="NUmber" class="form-label fw-bold">city:</label>
                    <input type="text" class="form-control" id="city" name="landmark">
                    <p id="city_error" class="error"></p>
                </div>
                <div class="col-md-6">
                    <label for="NUmber" class="form-label fw-bold">district:</label>
                    <input type="text" class="form-control" id="district" name="district">
                    <p id="district_error" class="error"></p>
                </div>
                <div class="col-md-6">
                    <label for="NUmber" class="form-label fw-bold">state:</label>
                    <input type="text" class="form-control" id="state" name="state">
                    <p id="state_error" class="error"></p>
                </div>
                <div class="col-md-6">
                    <label for="NUmber" class="form-label fw-bold">zipcode:</label>
                    <input type="Number" class="form-control" id="zipcode" name="zipcode">
                    <p id="zipcode_error" class="error"></p>
                </div>
                <div class="col-md-6">
                    <label for="orderId" class="form-label fw-bold">orderId:</label>
                    <input type="text" readonly class="form-control" id="orderId" name="orderId" value='#{Data.id}'>
                    <p id="orderId_err" class="error"></p>
                </div>
                <div class="col-md-6">
                    <label for="orderId" class="form-label fw-bold">Product Name:</label>
                     <input type="text" readonly  class="form-control"id="productName" name="productName" value='#{Data.productName}'>
                    <p id="orderId_err" class="error"></p>
                </div>
                <div class="col-md-6">
                    <label for="orderId" class="form-label fw-bold">Product price:</label>
                    <input type="text" readonly class="form-control" id="orderPrice" name="orderPrice" value='#{Data.price}'>
                    <p id="orderId_err" class="error"></p>
                </div>




                <div class="col-12">
                    <button type="submit" class="btn btn-primary">Book Now</button>
                </div>
            </form>




        </div>
